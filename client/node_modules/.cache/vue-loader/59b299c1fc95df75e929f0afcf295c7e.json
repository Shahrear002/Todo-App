() {\n    return {\n      form: { name: \"\", password: \"\" },\n    };\n  },\n  methods: {\n    login() {\n      const { name, password } = this.form;\n      if (!name || !password) {\n        alert(\"Username and password are required\");\n      }\n      try {\n        const {\n          data: { token },\n        } = axios.post(`${BASE_URL}/users/login`, {\n          name,\n          password,\n        });\n        localStorage.setItem(\"token\", token);\n        this.$router.push(\"/todo\");\n      } catch (error) {\n        alert(\"Invalid username or password.\");\n      }\n    },\n  },\n};\n</script>"]}]}